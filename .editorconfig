# EditorConfig file
# https://editorconfig.org/

# C settings
[*.c]
indent_style = space
indent_size = 2
tab_width = 2
end_of_line = lf
insert_final_newline = true
trim_trailing_whitespace = true
max_line_length = 80
charset = utf-8

# Go settings
[*.go]
indent_style = tab
indent_size = 2
tab_width = 2
end_of_line = lf
insert_final_newline = true
trim_trailing_whitespace = true
max_line_length = 80
charset = utf-8
